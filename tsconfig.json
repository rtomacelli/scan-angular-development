{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@environments/*": ["environments/*"],
      "@guards/*": ["scan/guards/*"],
      "@helpers/*": ["scan/helpers/*"],
      "@models/*": ["scan/models/*"],
      "@routes/*": ["scan/routes/*"],
      "@modules/*": ["scan/modules/*"],
      "@services/*": ["scan/services/*"],
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
